




<%= simple_form_for restaurant do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :address, "Address" %>
  <%= f.text_field :address %>
  <%= f.labal :phone_number, "Phone number" %>
  <%= f.text_field :phone_number %>
  <%= f.input :category, :collection => ["chinese", "italian", "japanese", "french", "belgian"] %>
  <%= f.submit "Submit" %>
<% end %>